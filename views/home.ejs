<%- include('partials/header'); -%>
<div class="container">
  <h1>Home</h1>
  <p><%= startingContent %></p>
  <% posts.forEach(function(post) {%>
  <h1><%= post.title %></h1>
  <p>
    <%= post.content.substring(0, 30) + ' ...' %>
    <a href="/posts/<%= post.title %>">Read more</a>
  </p>
  <% }) %>
</div>
<%- include('partials/footer'); -%>
